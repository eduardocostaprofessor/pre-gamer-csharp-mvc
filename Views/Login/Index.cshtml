@model Jogador

@{
    ViewData["Title"] = "Login";
}

<h1>Tela de Login</h1>
<h2>@TempData.Peek("Message")</h2>

<form method="post" action="@Url.Action("Logar")">
    <label for="">Email</label>
    <input type="email" name="Email" value="edu@edu.com">
    
    <label for="">Senha</label>
    <input type="password" name="Senha" value="1234">

    <button>Logar</button>
</form>